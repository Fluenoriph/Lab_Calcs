x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="111"/><column index="2" value="111"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="ventilation" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="39"/><column index="2" value="41"/><column index="3" value="44"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="vibration" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="39"/><column index="2" value="41"/><column index="3" value="44"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE VIEW rad_register AS
SELECT rad_reg_data.protocol_number, rad_reg_data.research_date, rad_reg_data.release_date, 
rad_reg_data.object_name, rad_reg_data.city, rad_reg_data.address, protocol_type.work_type,
gamma_radiation.true AS gamma_radiation_ok, gamma_radiation.false AS gamma_radiation_no,
radon_volume_activity.true AS radon_volume_activity_ok, radon_volume_activity.false AS radon_volume_activity_no,
eeva_radon.true AS eeva_radon_ok, eeva_radon.false AS eeva_radon_no,
radon_flux_density.true AS radon_flux_density_ok, radon_flux_density.false AS radon_flux_density_no,
employees.name

FROM rad_reg_data
JOIN protocol_type ON protocol_type.id = rad_reg_data.type_id
JOIN gamma_radiation ON gamma_radiation.id = rad_reg_data.id
JOIN radon_volume_activity ON radon_volume_activity.id = rad_reg_data.id
JOIN eeva_radon ON eeva_radon.id = rad_reg_data.id
JOIN radon_flux_density ON radon_flux_density.id = rad_reg_data.id
JOIN employees ON employees.id = rad_reg_data.employee_id</sql><current_tab id="0"/></tab_sql></sqlb_project>
